{
    "name": "remove-comments",
    "displayName": "Remove Comments",
    "description": "Remove all comments from your code at once!",
    "version": "0.0.1",
    "publisher": "plibither8",
    "license": "MIT",
    "author": {
        "name": "Aaron Bond"
    },
    "homepage": "https://github.com/plibither8/vscode-remove-comments/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/plibither8/vscode-remove-comments"
    },
    "bugs": "https://github.com/plibither8/vscode-remove-comments/issues",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Formatters"
    ],
    "keywords": [
        "comment", "comments", "delete", "remove"
    ],
    "activationEvents": [
        "onCommand:extension.removeComments"
    ],
    "gallaryBanner": {

    },
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "title": "Better Comments configuration",
            "properties": {
                "remove-comments.multilineComments": {
                    "type": "boolean",
                    "default": true
                }
            }
        },
        "commands": [
            {
                "command": "extension.removeComments",
                "title": "Remove all comments"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
